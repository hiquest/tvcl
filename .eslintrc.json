{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-use-before-define": [2, { "functions": false } ],
    "semi": [2, "never"],
    "no-underscore-dangle": 0,
    "no-return-assign": 0,
    "camelcase": 0,
    "no-console": 0
  }
}
